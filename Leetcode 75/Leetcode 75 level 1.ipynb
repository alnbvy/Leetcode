    "# 1480. Running Sum of 1d Array\n",
    "\n",
    "class Solution(object):\n",
    "    def runningSum(self, nums):\n",
    "        \"\"\"\n",
    "        :type nums: List[int]\n",
    "        :rtype: List[int]\n",
    "        \"\"\"\n",
    "        return [sum(nums[:i+1]) for i in range(len(nums))]"
   ]
  }
